            ---------------------------------------------------------
              queryCheckReservation(){
                ...
                this.sites.map((data,key)=>{
                    if(key==0){
                        sitesId += data.id
                        resources += this.state.cpu[key]+','+this.state.mem[key]+','+this.state.ADD--ATTRIBUTE--NAME[key]
                    }else{
                        sitesId += ','+data.id
                        resources += '|'+this.state.cpu[key]+','+this.state.mem[key]+','+this.state.ADD--ATTRIBUTE--NAME[key]
                    }
                })
                ...
              }
            ---------------------------------------------------------
