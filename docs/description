              let {cpu,mem} = this.state
              cpu[index] = ''
              mem[index] = ''
              ADD--ATTRIBUTE--NAME[ADD--ATTRIBUTE--NAME] = ''
              this.setState({
                  cpu: cpu,
                  mem: mem,
                  ADD--ATTRIBUTE--NAME: ADD--ATTRIBUTE--NAME
              })
